<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="item_layout" format="reference|integer" />

    <declare-styleable name="custom_navigation_view">
        <attr name="item_layout" />
    </declare-styleable>

    <declare-styleable name="hare_array_view">
        <attr name="item_layout" />
        <attr name="isMultiSelected" format="boolean" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>